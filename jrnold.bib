@Unpublished{chapman2011_condit_natur_imf_catal_effec,
  Title                    = {The Conditional Nature of the IMF Catalytic Effect},
  Author                   = {Terrence Chapman and Songying Fang and Randall W. Stone},
  Date                     = {2011-04-16},
  Year                     = {2011},
  Note                     = {working paper},

  Abstrct                  = { How do international financial instituitons interact with international markets? In particular, how to markets react to major multilateral lending initiatives. We analyze a formal model that demonstrates that the âcatalytic effectâ of the IMF, or the ef- fect of IMF program announcements on international investment flows, is conditional. Namely, the degree to which IMF programs stimulate investment flows depends on how market actors interpret the motivations behind a lending decision of the IMF. If investors believe the IMF endorsement of a program as a sign of economic weak- ness of a country, then they do not invest following the IMF decision. On the other hand, if the market actors interpret an IMF endorsement as a sign of major donorsâ interest in the applicant country, then they may respond with private investment, cre- ating the âcatalytic effect.â In other words, informal influence driving IMF decisions can have differing implications for the likelihood of future certainty in the investment environment. Our theory helps explain discrepant empirical findings with respect to the catalytic effect of IMF programs, and we test additional empirical implications by examining how sovereign credit ratings are affected by program announcements, loan size, and scope of conditions attached to loans. },
  Owner                    = {jrnold}
}

