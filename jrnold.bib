@Article{KoopPotter2004,
  Title                    = {Estimating Multiple Change-Point Models with Unknown Number of Change Points},
  Author                   = {Gary M. Koop and Simon M. Potter},
  Journaltitle             = {Federal Reserve Bank of New York Staff Reports},
  Year                     = {2004},
