  Note                     = {working paper},

  Abstract                 = {Political scientists have examined how the competition for capital affects the setting of national tax rates. Yet taxes levied on firms are often subject to special exemptions that are negotiated between government actors and the firm. In this paper I explore how political institutions, specifically the level of democracy, affect firm-level tax setting across the world. While democratic institutions can affect tax setting through a number of mechanisms, the overall relationship is an empirical question. Using a confidential data set on firm tax payments on the foreign affiliates of U.S. Multinational Corporations from the U.S. Bureau of Economic Analysis I show that there are large variations within countries on the tax burdens faced by firms. I find evidence that the mobility of the specific investment project, the types of spillovers these investments provide to a community, and attributes of the parent firm are all important determinants of taxation. While firm level factors clearly affect corporate taxation, I argue that democratic institutions constrain the ability of governments to offer special tax deals to multinational corporations. After controlling for parent firm and foreign levels factors I find that democratic institutions greatly reduce the likelihood of firms receiving tax exemptions. This leads to firms paying on average 20\% more corporate taxes in democratic regimes.},
  Owner                    = {jrnold},
  Tags                     = {rochester-mnc},
  Timestamp                = {2010.05.04}
}

@Unpublished{JensenMartinEtAl2007,
