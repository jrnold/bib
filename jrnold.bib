@Article{BuysseReynoldsEtAl1989,
  Title                    = {The Pittsburgh Sleep Quality Index: a new instrument for psychiatric practice and research.},
  Author                   = {D. J. Buysse and C. F. Reynolds and T. H. Monk and S. R. Berman and D. J. Kupfer},
  Year                     = {1989},
