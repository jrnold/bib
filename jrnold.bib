@Unpublished{Wolford2009,
  Title                    = {Leadership Turnover as a Commitment Problem},
  Author                   = {Wolford, Scott},
  Year                     = {2009},
  Month                    = jan,
  Note                     = {working paper},
  Url                      = {http://spot.colorado.edu/~wolfordm/shadow5.pdf},

  Owner                    = {jrnold},
  Review                   = {How does the expected behavior of an incumbent leaderâs successor affect bargain- ing outcomes in the present? I analyze a game-theoretic model of crisis bargaining in which (a) the distributive nature of settlements affects the probability of political survival, (b) leadership turnover implies the accession of a new leader with potentially different preferences for war, and (c) successors may renegotiate inherited settlements. Commitment problems thus arise endogenously and may be solved in one of two ways. First, incumbents otherwise willing to make large concessions may be attacked, dragged into wars by very resolute successors who will revise settlements in the future. Sec- ond, moderately resolute successors can lend bargaining power to otherwise irresolute incumbents, who can demand concessions that bolster their hold on political power. Examining leadership turnover as a commitment problem thus allows for a considera- tion of the effects of the future balance of resolve on present bargaining outcomes.},
  Tags                     = {PSC577},
  Timestamp                = {2009.01.18}
}

@Unpublished{WolfordReiterEtAl2008,
  Title                    = {Information, Commitment, and War},
  Author                   = {Wolford, Scott and Reiter, Dan and Carrubba, Cliff},
  Year                     = {2008},
  Month                    = oct,
  Url                      = {http://spot.colorado.edu/~wolfordm/wrc_12.pdf},

  Abstract                 = {Most formal scholarship explains war as a function of either asymmetric information or credible commitment problems. We present the first bargaining model of war that allows for both asymmetric information and commitment problems. The model produces three striking results, all of which emerge when both asymmetric information and commitment problems are present. First, in contrast to some existing information-oriented models, the resolution of uncertainty through fighting can lead to the continuation of war rather than its termination. Second, wars are less---not more---likely to end the longer they have lasted. Third, a belligerent will increase its war aims over time as it becomes convinced that it faces an unappeasable opponent. Finally, the dynamics that characterize wars in purely asymmetric information or commitment models should exist only when the other bargaining problem is absent or trivial, a condition that may be satisfied only rarely in an anarchic international system.},
  Owner                    = {jrnold},
  Tags                     = {PSC577},
  Timestamp                = {2009.01.15}
}

