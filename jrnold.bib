  Timestamp                = {2009-06-03}
}

@Article{VerwimpJustinoEtAl2009,
  Title                    = {{The Analysis of Conflict: A Micro-Level Perspective}},
  Author                   = {Verwimp, Philip and Justino, Patricia and Bruck, Tilman},
  Year                     = {2009},
  Doi                      = {10.1177/0022343309102654},
  Eprint                   = {http://jpr.sagepub.com/cgi/reprint/46/3/307.pdf},
