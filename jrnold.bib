@Article{GohOhJeongEtAl2002,
  Title                    = {Classification of Scale-Free Networks},
  Author                   = {Goh, Kwang-Il and Oh, E. and Jeong, H. and Kahng, B. and Kim, D.},
  Year                     = {2002},
  Note                     = {???Defines betweenness centrality measure (static version of load). ???Classifies scale-free networks by power-law exponent of betweenness (??) distribution. ???Defines Mass-Distance, M(d), function as number of nodes within a distance. Shows topology of shortest paths: tree-like or blob. ???These have implications on attack vulnerability ???Class I: ?? is 2.0 and non-monotonic M(d). Less vulnerable to attack. ???Class IIa: ?? is 2.2 and linear M(d) with slope A>1 for large d. Less vulnerable to attack because shortest paths multiply connected. ???Class IIb: ?? is 2.2 and linear M(d) with slope A~1 for large d. More vulnerable to attack because shortest paths singly connected.},
  Number                   = {20},
  Pages                    = {12583-12588},
  Volume                   = {99},

  Abstract                 = {While the emergence of a power law degree distribution in complex networks is intriguing, the degree exponent is not universal. Here we show that the betweenness centrality displays a power-law distribution with an exponent \eta which is robust and use it to classify the scale-free networks. We have observed two universality classes with \eta \approx 2.2(1) and 2.0, respectively. Real world networks for the former are the protein interaction networks, the metabolic networks for eukaryotes and bacteria, and the co-authorship network, and those for the latter one are the Internet, the world-wide web, and the metabolic networks for archaea. Distinct features of the mass-distance relation, generic topology of geodesics and resilience under attack of the two classes are identified. Various model networks also belong to either of the two classes while their degree exponents are tunable.},
  Journal                  = {Proceedings of the National Academy of Sciences of the United States of America (PNAS)},
  Owner                    = {jrnold}
}

