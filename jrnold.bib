@Article{PlowesAdams2005,
  Title                    = {An empirical test of {Lanchester's} square law: mortality during battles of the fire ant Solenopsis invicta},
  Author                   = {Plowes, Nicola J.R and Adams, Eldridge S},
  Year                     = {2005},
  Doi                      = {10.1098/rspb.2005.3162},
