@Article{lemarchand1998genoc,
  Title                    = {Genocide in the Great Lakes: Which Genocide? Whose Genocide?},
  Author                   = {Lemarchand, Ren\'{e}},
  Year                     = {1998},
  ISSN                     = {00020206},
  Language                 = {English},
