@Article{LatoraMarchiori2002,
  Title                    = {Is the {Boston} Subway System a Small-World Network},
  Author                   = {Latora, Vito and Marchiori, Massimo},
  Year                     = {2002},
  Note                     = {Study of the Boston subway system Problems with topological abstraction when applied to real networks (esp. geographic component) Uses adjacency matrix weighted by geographic distance Efficiency of between nodes is 1 / distance making efficiency = 0 when distance = infinity (no edge) Define efficiency at both global and local levels (equivalents to clustering coefficient and avg path length) Define cost of the network with a fully connected network as max. Results: 63\% global efficiency 3\% local efficiency Only 0.2\% cost of fully connected network Subways (when considered as closed network, i.e. ignoring buses and othet substitutes) have high error tolerances. },
  Pages                    = {109-113},
  Volume                   = {314},

  Abstract                 = {The mathematical study of the small-world concept has fostered quite some interest, showing that small-world features can be identified for some abstract classes of networks. However, passing to real complex systems, as for instance transportation networks, shows a number of new problems that make current analysis impossible. In this paper we show how a more refined kind of analysis, relying on transportation efficiency, can in fact be used to overcome such problems, and to give precious insights on the general characteristics of real transportation networks, eventually providing a picture where the small-world comes back as underlying construction principle.},
  Journal                  = {Physica A},
  Owner                    = {jrnold}
}

