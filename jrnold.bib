@Inproceedings{snidal2011_infor_inter_organ_iigos,
  Title                    = {Informal Intergovernmental Organizations (IIGOs)},
  Author                   = {Duncan Snidal and Felicity Vabulas},
  Booktitle                = {Informal Governance in International Organizations},
  Date                     = {2011-04-16},
  Year                     = {2011},

  Abstract                 = { The recent renaissance in the theoretical analysis of intergovernmental organizations has focused on formal IGOsâlegalized interstate arrangements coordinated through permanent secretariats with varying degrees of autonomy. But many intergovernmental organizations are informal in natureâthey are not subject to a formal treaty and/or do not have a permanent secretariat. Important examples include the various G-groups that are the locus of much high-level interaction among states. What does the broader set of informal intergovernmental organizations (IIGO) look like? How can they best be conceptualized in relation to the full spectrum of international institutions? Why would states choose to create and work through an IIGO rather than a formal IGO? We build on the analysis of soft law, informal agreements, and informal governance to understand why IIGOs are important in their own right and are not just stepping stones to more formal organizationsâalthough we are also interested in that possibility. IIGOs act as complements for formal IGOs when states face high uncertainty surrounding other statesâ preferences and the state of the world; IIGOs offer a low risk forum for creating focal point solutions that can be implemented through independent state action. IIGOs act as substitutes for formal IGOs when states need increased flexibility, speed, ambiguity or weaker enforcement. We place IIGOs along a spectrum of intergovernmental arrangements with varying degrees of formality and we supplement this theoretical analysis by developing a comprehensive list of informal intergovernmental organizations. },
  Institution              = {University of Rochester},
  Owner                    = {jrnold}
}

