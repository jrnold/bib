  Pages                    = {934-950},
  Url                      = {http://jcr.sagepub.com/cgi/content/abstract/53/6/934},
  Volume                   = {53},

  Abstract                 = {In a much-cited recent article, Obermeyer, Murray, and Gakidou (2008a) examine estimates of wartime fatalities from injuries for thirteen countries. Their analysis poses a major challenge to the battle-death estimating methodology widely used by conflict researchers, engages with the controversy over whether war deaths have been increasing or decreasing in recent decades, and takes the debate over different approaches to battle-death estimation to a new level. In making their assessments, the authors compare war death reports extracted from World Health Organization (WHO) sibling survey data with the battle-death estimates for the same countries from the International Peace Research Institute, Oslo (PRIO). The analysis that leads to these conclusions is not compelling, however. Thus, while the authors argue that the PRIO estimates are too low by a factor of three, their comparison fails to compare like with like. Their assertion that there is "no evidence" to support the PRIO finding that war deaths have recently declined also fails. They ignore war-trend data for the periods after 1994 and before 1955, base their time trends on extrapolations from a biased convenience sample of only thirteen countries, and rely on an estimated constant that is statistically insignificant.},
  Journal                  = {Journal of Conflict Resolution},
  Owner                    = {jrnold},
  Timestamp                = {2009.12.20}
}

@Unpublished{Spaniel2010,
  Title                    = {Potential Power: Extracting Concessions without Proliferation},
  Author                   = {William Spaniel},
  Date                     = {2010-11-05},
  Year                     = {2010},
  Month                    = {11},
  Note                     = {working paper},

  Abstract                 = {Deciding to build nuclear weapons is tricky because development is costly and risks provoking military intervention. Current models of pre- ventive war overlook the endogenous choice and the development costs a proliferator incurs. In this article, I construct an endogenous power shift model that incorporates the cost of proliferation. There are three major findings. (1) If the declining state would declare war in response to a power shift, the rising state does not build. (2) The rising state is only willing to proliferate if the additional concessions it will extract are greater than its development costs. (3) In some cases, the rising state ex- tracts concessions without proliferating. Surprisingly, states can use the potential for powerânot just fully realized powerâas a form of bargaining leverage.},
  Owner                    = {jrnold},
  Timestamp                = {2010.11.02}
}

