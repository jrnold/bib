@Article{WandShottsEtAl2001,
  Title                    = {The Butterfly Did It: The Aberrant Vote for Buchanan in Palm Beach County, Florida},
  Author                   = {Wand, Jonathan N. and Shotts, Kenneth W. and Sekhon, Jasjeet S. and Mebane, Walter R. Jr and Herron, Michael C. and Brady, Henry E.},
