@Article{welty2009bayes,
  Title                    = {Bayesian Distributed Lag Models: Estimating Effects of Particulate Matter Air Pollution on Daily Mortality},
  Author                   = {Welty, L. J. and Peng, R. D. and Zeger, S. L. and Dominici, F.},
  Year                     = {2009},
