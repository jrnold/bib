@Inproceedings{WillingerAldersonDoyleEtAl2004,
  Title                    = {More ``Normal'' than Normal: Scaling Distributions and Complex Systems},
  Author                   = {Walter Willinger and David Alderson and John C. Doyle and Lun Li},
  Booktitle                = {Proceedings of the 2004 Winter Simulation Conference},
