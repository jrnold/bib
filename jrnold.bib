@Article{WiperPettitEtAl2000,
  Title                    = {{Bayesian} Inference for a {Lanchester} Type Combat Model},
  Author                   = {Wiper, M. and , L. and Young, K.},
  Year                     = {2000},
  Doi                      = {10.1002/1520-6750(200010)47:7<541::AID-NAV1>3.0.CO;2-0},
