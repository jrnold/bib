@Article{salim2001bayes,
  Title                    = {A {Bayesian} Stochastic Formulation of {Lanchester} Combat Theory},
  Author                   = {Salim Al-khalidi, Abdul Sattar Rashid and Hamid, W. M.},
  Year                     = {2001},
  Doi                      = {doi:10.5711/morj.6.3.69},
