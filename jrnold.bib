@Unpublished{MangerPickupEtAl,
  Title                    = {A Hierarchy of Preferences: A Longitudinal Network Analysis Approach to PTA Formation},
  Author                   = {Manger, Mark S. and Pickup, Mark A. and Snijders, Tom},
  Note                     = {working paper},

  Abstract                 = {Preferential trade agreements have proliferated rapidly within the last 15 years, growing into a dense network of multiple ties between countries. The spread of PTAs, however, is not uniform: some countries have signed a multitude of deals, while oth- ers remain much less involved. Certain countries manage to position themselves as hubs with ties spanning the globe, yet others are at most connected to their immediate neighbours. This paper presents a longitudinal network analysis method to analyze the patterns of the formation of trade agreements, based on the mutual co-determination of network structure and actor choices. The findings suggest that preferential trade agreements establish a clear hierarchy among countries: Developed countries avoid agreements with each other, form ties with middle-income countries, but shun the least developed members of the system. Middle-income countries establish a horizontal level of PTAs in the system that is characterized by regionally concentrated subnetworks. Supplanting the multilateral trade regime with preferential agreements therefore cre- ates a system of highly asymmetrical relationships of weaker spokes around a few hubs, while potentially leaving the weakest members behind.},
  Owner                    = {jrnold},
  Timestamp                = {2009.07.13}
}

